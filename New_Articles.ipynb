   "outputs": [],
   "source": [
    "\n",
    "# create a list of dictionaries of all NYT articles containing oil or oilrelated data from 2010 to 2019\n",
    "all_years    = list(range(2010,2020))\n",
    "all_articles = call_nyt_by_all_years(all_years)"
   ]
